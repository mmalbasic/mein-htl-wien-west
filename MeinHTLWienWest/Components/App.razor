    <script>
        window.getImageContainerRect = function (id) {
            const el = document.getElementById(id);
            const rect = el.getBoundingClientRect();
            return {
                left: rect.left,
                top: rect.top,
                width: rect.width,
                height: rect.height
            };
        };
